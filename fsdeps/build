#!/bin/sh
# This file is automatically generated by fs-package

set -e

. fsdeps/env.sh
. fsdeps/system.sh
. fsdeps/arch.sh

echo FSDEPS_PREFIX=$FSDEPS_PREFIX
echo LIBRARY_PATH=$LIBRARY_PATH
echo PKG_CONFIG_PATH=$PKG_CONFIG_PATH
if [ "`uname`" = "Darwin" ]; then
echo MACOSX_DEPLOYMENT_TARGET=$MACOSX_DEPLOYMENT_TARGET
fi

sh fsdeps/build.sh

if [ "$SYSTEM" = "Windows" ]; then
echo "mkdir -p fsdeps/_dll"
mkdir -p fsdeps/_dll
echo "cp $FSDEPS_PREFIX/bin/*.dll fsdeps/_dll"
cp $FSDEPS_PREFIX/bin/*.dll fsdeps/_dll
fi

echo "Built fsdeps for ${SYSTEM}_${ARCH}"
